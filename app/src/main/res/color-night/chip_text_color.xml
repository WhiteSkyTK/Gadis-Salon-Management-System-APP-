<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State when the chip is selected (checked) -> White Text -->
    <item android:color="@color/textColorPrimary" android:state_checked="true" />
    <!-- Default state (not checked) -> Light Text for dark theme -->
    <item android:color="@color/textColorPrimary" />
</selector>
